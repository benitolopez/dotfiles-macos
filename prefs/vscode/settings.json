{
  "editor.fontSize": 16,
  "editor.fontFamily": "Hack",
  "editor.wordWrap": "on",
  "editor.letterSpacing": 0.5,
  "editor.renderWhitespace": "selection",
  "editor.insertSpaces": false,
  "intelephense.diagnostics.undefinedFunctions": false,
  "intelephense.diagnostics.undefinedTypes": false,
  "intelephense.diagnostics.undefinedConstants": false,
  "telemetry.telemetryLevel": "off",
  // "search.exclude": {
  //   "**/dist": true
  // },
  "security.workspace.trust.untrustedFiles": "open",
  "editor.guides.indentation": false,
  "window.openFoldersInNewWindow": "on",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "terminal.integrated.defaultProfile.linux": "",
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer",
    "editor.formatOnSave": true
  },
  "workbench.colorTheme": "GitHub Light",
  "editor.lineHeight": 2.2,
  "editor.bracketPairColorization.enabled": false,
  "workbench.colorCustomizations": {
    "terminal.background": "#000000",
    "[Braver's Solarized Light]": {
      "editorLineNumber.foreground": "#94A1A1",
      "editorLineNumber.activeForeground": "#657B83",
      "editorGutter.background": "#EFE8D8",
      "editorWhitespace.foreground": "#94A1A1",
      "editor.snippetTabstopHighlightBackground": "#EFE8D8"
    },
    "[Braver's Solarized Dark]": {
      "editorGutter.background": "#0A3743",
      "editor.snippetTabstopHighlightBackground": "#0A3743"
    },
    "[Braver's Solarized Light (themed workbench)]": {
      "editorLineNumber.foreground": "#94A1A1",
      "editorLineNumber.activeForeground": "#657B83",
      "editorGutter.background": "#EFE8D8",
      "editorWhitespace.foreground": "#94A1A1",
      "editor.snippetTabstopHighlightBackground": "#EFE8D8"
    },
    "[GitHub Light]": {
      "editor.background": "#f3f5f7"
    }
  },
  "workbench.preferredDarkColorTheme": "GitHub Dark",
  "workbench.preferredLightColorTheme": "GitHub Light",
  "search.mode": "reuseEditor",
  // "files.exclude": {
  //   "**/dist": true
  // },
  "editor.showUnused": false,
  "[go]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "golang.go"
  },
  "workbench.tree.indent": 20,
  "vsicons.dontShowNewVersionMessage": true,
  "php.validate.executablePath": "/opt/homebrew/bin/php",
  "intelephense.telemetry.enabled": false,
  "intelephense.stubs": [
    "apache",
    "bcmath",
    "bz2",
    "calendar",
    "com_dotnet",
    "Core",
    "ctype",
    "curl",
    "date",
    "dba",
    "dom",
    "enchant",
    "exif",
    "FFI",
    "fileinfo",
    "filter",
    "fpm",
    "ftp",
    "gd",
    "gettext",
    "gmp",
    "hash",
    "iconv",
    "imap",
    "intl",
    "json",
    "ldap",
    "libxml",
    "mbstring",
    "meta",
    "mysqli",
    "oci8",
    "odbc",
    "openssl",
    "pcntl",
    "pcre",
    "PDO",
    "pdo_ibm",
    "pdo_mysql",
    "pdo_pgsql",
    "pdo_sqlite",
    "pgsql",
    "Phar",
    "posix",
    "pspell",
    "readline",
    "Reflection",
    "session",
    "shmop",
    "SimpleXML",
    "snmp",
    "soap",
    "sockets",
    "sodium",
    "SPL",
    "sqlite3",
    "standard",
    "superglobals",
    "sysvmsg",
    "sysvsem",
    "sysvshm",
    "tidy",
    "tokenizer",
    "xml",
    "xmlreader",
    "xmlrpc",
    "xmlwriter",
    "xsl",
    "Zend OPcache",
    "zip",
    "zlib",
    "wordpress"
  ],
  "intelephense.environment.includePaths": ["../libraries"],
  "vsicons.presets.foldersAllDefaultIcon": true,
  "explorer.compactFolders": false,
  "files.insertFinalNewline": true,
  "intelephense.diagnostics.typeErrors": false,
  "editor.formatOnType": true,
  "editor.wrappingIndent": "indent",
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "workbench.editor.highlightModifiedTabs": true,
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "git.openRepositoryInParentFolders": "never",
  "workbench.activityBar.location": "hidden",
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false,
    "scss": false,
    "go": false
  },
  "[cpp]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "C_Cpp.clang_format_fallbackStyle": "Google",
  "[python]": {
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnSave": true
  }
}
